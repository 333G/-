@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = $.request("keyValue");//F_Id

    $(function () {
        if (!!keyValue) {
            $.ajax({
                url: "/SMCManage/SMCSend/GetFormJson",
                data: { keyValue: keyValue },
                dataType: "json",
                async: false,
                success: function (data) {
                    $("#form1").formSerialize(data);
                }
            });
        }

    });
</script>
<title></title>
<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <div style="padding-top: 20px; margin-right: 30px;">
            <table class="form">
                <tr>
                    <th class="formTitle" style="padding-right:8px;">发送手机号码:</th>
                    <td class="formValue" style="width:100%;">
                        <textarea id="F_MobileList" name="F_MobileList" readonly class="form-control required" style="height: 100px;width: 100%;"></textarea>
                    </td>
                </tr>
                <tr>
                    <th class="formTitle" style="padding-right:8px;">发送短信内容:</th>
                    <td class="formValue" style="width:100%;">
                        <textarea id="F_SmsContent" name="F_SmsContent" readonly class="form-control required" style="height: 150px;width:100%;"></textarea>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</form>
